{
  "name": "vuepress-theme-yur",
  "version": "2.0.6",
  "description": "VuePress Theme - Yur",
  "keywords": [
    "theme",
    "vuepress",
    "yur"
  ],
  "homepage": "https://github.com/cnguu/vuepress-theme-yur/",
  "bugs": {
    "url": "https://github.com/cnguu/vuepress-theme-yur/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnguu/vuepress-theme-yur.git"
  },
  "license": "MIT",
  "author": "cnguu <www@cnguu.cn>",
  "main": "./yur/index.js",
  "files": [
    "yur/"
  ],
  "scripts": {
    "build": "vuepress build docs",
    "dev": "vuepress dev docs",
    "lint": "eslint --ext .js,.vue yur docs",
    "publish": "git push origin master:master --tags",
    "serve": "vuepress serve docs",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "ant-design-vue": "^1.4.10",
    "crypto-js": "^3.1.9-1",
    "ityped": "^1.0.3",
    "js-cookie": "^2.2.1",
    "leancloud-storage": "^4.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "transliteration": "^2.1.8",
    "valine": "^1.3.10",
    "vuepress": "^1.3.0",
    "vuepress-plugin-container": "^2.1.2",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vssue/api-github-v3": "^1.4.0",
    "@vssue/vuepress-plugin-vssue": "^1.4.2",
    "babel-eslint": "^10.0.3",
    "conventional-changelog-cli": "^2.0.28",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "sort-package-json": "^1.39.1",
    "vuepress-plugin-serve": "^2.0.0"
  }
}
